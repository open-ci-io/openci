{
	"dart.flutterSdkPath": ".fvm/versions/stable",
	"editor.codeActionsOnSave": {
		"source.fixAll": "explicit"
	},
	"editor.formatOnSave": true,
	"eslint.workingDirectories": [
		{
			"mode": "auto"
		}
	],
	"files.exclude": {
		"**/*.freezed.dart": true,
		"**/*.g.dart": true
	},
	"files.watcherExclude": {
		"**/.fvm": true
	},
	"search.exclude": {
		"**/.fvm": true
	}
}
